{
  "name": "tonyvlcek/tu-csb-mymo",
  "description": "Benchmark evaluating the performance of using MySQL vs. MongoDB as a document store.",
  "keywords": [
    "mysql",
    "mongodb",
    "php",
    "benchmark",
    "document-store"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "ext-mongodb": "*",
    "ext-pdo": "*",

    "contributte/bootstrap": "^0.5.0",
    "contributte/di": "^0.5.0",
    "contributte/utils": "^0.5.0",
    "contributte/latte": "^0.5.0",
    "contributte/tracy": "^0.5.0",
    "contributte/console": "^0.9.0",

    "nette/robot-loader": "^3.4.0",
    "mongodb/mongodb": "^1.15",
    "google/cloud-storage": "^1.30",
    "fakerphp/faker": "^1.21"
  },
  "require-dev": {
    "ninjify/qa": "^0.13",
    "ninjify/nunjuck": "^0.4",
    "contributte/dev": "^0.3.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-doctrine": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
